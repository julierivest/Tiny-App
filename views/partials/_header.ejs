<header>
  <h1>URL Shortener</h1>
    <% if (!username) { %>
      <form action="/login" method="POST">
        <input type="text" name="username" style="width: 300px">
        <input type="submit" value="Sign In">
      </form>
    <%} else {%>
      <span> <%=username%> </span>
      <% } %>

</header>


